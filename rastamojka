def tamoj_oform(cost):          # таможенное оформление, цена автомобиля в рублях, не зависит от типа автомобиля
    if cost <= 200000:
        return 775
    elif cost <= 450000:
        return 1550
    elif cost <= 1 200 000:
        return 3 100
    elif cost <= 2 700 000:
        return 8 530
    elif cost <= 4 200 000:
        return 12 000
    elif cost <= 5 500 000:
        return 15 500
    elif cost <= 7 000 000:
        return 20 000
    elif cost <= 8 000 000:
        return 23 000
    elif cost <= 9 000 000:
        return 25 000
    elif cost <= 10 000 000:
        return 27 000
    elif cost > 10 000 000:
        return 30 000
        
def util_sbor(age):              # утиль.сбор, возраст автомобиля, не зависит от типа автомобиля
    if age <= 3:
        return 20 000 * 0.17
    elif age > 3:
        return 20 000 * 0.26
        
