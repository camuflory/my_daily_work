from datetime import datetime
import telebot
token = 'your token'
bot = telebot.TeleBot(token)
@bot.message_handler(content_types=['text', 'photo'])
date = str(datetime.now())
year = int(date[:4])
age = year - int(input())        # возраст автомобиля, текущий год - год производства автомобиля
